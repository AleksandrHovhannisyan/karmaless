{
    "name": "Karmaless",
    "version": "1.0.0",
    "description": "A karma-free Reddit experience",
    "permissions": [
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.reddit.com/*"],
        "run_at": "document_start",
        "js": ["content.js"]
      }
    ],
    "options_page": "options/options.html",
    "manifest_version": 2
}

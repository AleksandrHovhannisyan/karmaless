{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "jasmine": true,
    "jest": true,
    "es6": true,
    "webextensions": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"]
  },
  "ignorePatterns": "webpack.config.js"
}
